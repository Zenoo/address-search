{
  "name": "address-searcher",
  "title": "AddressSearch",
  "description": "Address searching with typeaheads and multiple choices",
  "keywords": [
    "address",
    "search",
    "google",
    "autocomplete",
    "prediction",
    "input",
    "typeahead"
  ],
  "scripts": {
    "compile": "babel address-search.js -o address-search-ie.min.js && browserify address-search-ie.min.js | uglifyjs > address-search.min.js && rm address-search-ie.min.js && jsdoc . -R README.md -c docs/jsdoc-conf.json",
    "release": "npm run compile && git add address-search.min.js && git diff-index --quiet HEAD || git commit -m \"Minification\" && standard-version"
  },
  "version": "1.12.4",
  "main": "address-search.js",
  "author": {
    "name": "Zenoo",
    "url": "https://github.com/Zenoo"
  },
  "maintainers": [
    {
      "name": "Zenoo",
      "url": "https://github.com/Zenoo"
    }
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "bugs": "https://github.com/Zenoo/address-search/issues",
  "homepage": "https://github.com/Zenoo/address-search",
  "docs": "https://github.com/Zenoo/address-search#readme",
  "download": "https://github.com/Zenoo/address-search/archive/master.zip",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "babel-preset-minify": "^0.5.1",
    "browserify": "^17.0.0",
    "core-js": "^3.15.2",
    "standard-version": "^9.3.0",
    "uglifyjs": "^2.4.11"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "section": "Visual updates"
      },
      {
        "type": "refactor",
        "section": "Refactoring"
      },
      {
        "type": "perf",
        "section": "Performance"
      },
      {
        "type": "progress",
        "section": "Progress"
      },
      {
        "type": "test",
        "section": "Tests"
      }
    ]
  }
}
