{
  "name": "address-searcher",
  "title": "AddressSearch",
  "description": "Address searching with typeaheads and multiple choices",
  "keywords": [
    "address",
    "search",
    "google",
    "autocomplete",
    "prediction",
    "input",
    "typeahead"
  ],
  "scripts": {
    "build": "babel address-search.js -o address-search.min.js && jsdoc . -R README.md -c docs/jsdoc-conf.json && git add -A . && git commit -m 'chore: Docs generation'",
    "release": "npm run build && standard-version"
  },
  "version": "1.12.6",
  "main": "address-search.js",
  "author": {
    "name": "Zenoo",
    "url": "https://github.com/Zenoo"
  },
  "maintainers": [
    {
      "name": "Zenoo",
      "url": "https://github.com/Zenoo"
    }
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "bugs": "https://github.com/Zenoo/address-search/issues",
  "homepage": "https://github.com/Zenoo/address-search",
  "docs": "https://github.com/Zenoo/address-search#readme",
  "download": "https://github.com/Zenoo/address-search/archive/master.zip",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-preset-minify": "^0.5.1",
    "jsdoc": "^3.6.7",
    "standard-version": "^9.3.0"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "section": "Visual updates"
      },
      {
        "type": "refactor",
        "section": "Refactoring"
      },
      {
        "type": "perf",
        "section": "Performance"
      },
      {
        "type": "progress",
        "section": "Progress"
      },
      {
        "type": "test",
        "section": "Tests"
      }
    ]
  }
}
